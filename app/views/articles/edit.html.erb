<div class="container mt-5 mb-5">
  <h3>Edit Article</h3>
  <%= form_with model: @article do |f| %>
    <%= f.label :title %>
    <br>
    <%= f.text_field :title %>
    <br>
    <%= f.label :content, class: "mt-3" %>
    <br>
    <%= f.text_area :content %>
    <br>
    <%= f.submit class: "btn btn-primary mt-2"%>
  <% end %>
</div>
